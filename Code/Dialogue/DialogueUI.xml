    <Frame name="DUIDialogIconFrameTemplate" mixin="DUIDialogIconFrameMixin" useParentLevel="false" virtual="true">
        <Size x="14" y="14"/>
        <Layers>
            <Layer level="OVERLAY" textureSubLevel="6">
                <Texture parentKey="Icon" setAllPoints="true"/>
            </Layer>
